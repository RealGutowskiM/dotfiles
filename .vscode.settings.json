{
	"activitusbar.toggleSidebar": true,
	"activitusbar.activeColour": "#1a8bfb",
	"activitusbar.inactiveColour": "#615F61",
	"editor.minimap.enabled": false,
	"editor.renderWhitespace": "none",
	"editor.folding": false,
	"editor.lineNumbers": "off",
	"editor.colorDecorators": true,
	"editor.useTabStops": true,
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"editor.occurrencesHighlight": false,
	"editor.fontSize": 16,
	"editor.fontFamily": "'SF Mono', Menlo, Monaco, 'Courier New', monospace",
	"editor.cursorSurroundingLines": 10,
	"explorer.openEditors.visible": 0,
	"html.mirrorCursorOnMatchingTag": false,
	"terminal.integrated.fontFamily": "'SF Mono', Menlo, Monaco, 'Courier New', monospace",
	"javascript.format.semicolons": "insert",
	"javascript.preferences.quoteStyle": "double",
	"vim.camelCaseMotion.enable": true,
	"vim.easymotion": true,
	"vim.sneak": true,
	"vim.useSystemClipboard": true,
	"vim.hlsearch": true,
	"vim.leader": ",",
	"vim.easymotionMarkerHeight": 22,
	"vim.easymotionMarkerWidthPerChar": 10,
	"vim.easymotionMarkerFontFamily": "'SF Mono', Menlo, Monaco, 'Courier New', monospace",
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<leader>",
				"f"
			],
			"commands": [
				"editor.action.formatDocument"
			]
		},
		{
			"before": [
				"<C-h>"
			],
			"commands": [
				"workbench.action.previousEditor"
			]
		},
		{
			"before": [
				"<C-l>"
			],
			"commands": [
				"workbench.action.nextEditor"
			]
		},
		{
			"before": [
				"<leader>",
				"w"
			],
			"commands": [
				"workbench.action.files.save"
			]
		},
		{
			"before": [
				"<space>"
			],
			"after": [
				"<leader>",
				"<leader>",
				"s"
			]
		}
	],
	"telemetry.enableTelemetry": false,
	"telemetry.enableCrashReporter": false,
	"window.nativeTabs": true,
	"window.titleBarStyle": "native",
	"workbench.activityBar.visible": false,
	"workbench.colorTheme": "MacOS Modern Light - Xcode Default",
	"workbench.editor.showIcons": false,
	"workbench.editor.tabCloseButton": "left",
	"workbench.startupEditor": "none",
	"workbench.settings.editor": "json",
	"workbench.list.keyboardNavigation": "simple",
	"workbench.list.automaticKeyboardNavigation": false,
	"workbench.iconTheme": "macos-modern-legit-xcode-icon-theme"
}