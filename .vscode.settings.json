{
  "editor.detectIndentation": false,
  "editor.insertSpaces": false,
  "editor.tabSize": 8,
  "editor.fontSize": 12,
  "editor.fontFamily": "'SF Mono', Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "'SF Mono', Menlo, Monaco, 'Courier New', monospace",
  "customizeUI.activityBar": "bottom",
  "customizeUI.titleBar": "inline",
  "customizeUI.listRowHeight": 20,
  "customizeUI.font.regular": "Helvetica Neue",
  "customizeUI.font.monospace": "SF Mono",
  "customizeUI.fontSizeMap": {
    "13px": "11px",
    "12px": "11px",
    "window-title": "12px",
    "tab-title": "12px",
    "monospace": "10.5px",
    "menu": "13px"
  },
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "window.titleBarStyle": "native",
  "window.nativeTabs": true,
  "window.zoomLevel": -0.5,
  "workbench.editor.tabCloseButton": "left",
  "workbench.editor.showIcons": false,
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "MacOS Modern Dark - Xcode Catalina",
  "workbench.settings.editor": "json",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "macos-modern-legit-finder-icon-theme",
  "workbench.list.keyboardNavigation": "simple",
  "workbench.list.automaticKeyboardNavigation": false,
  "vim.camelCaseMotion.enable": false,
  "vim.easymotion": false,
  "vim.sneak": false,
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.leader": ",",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.nextEditor"]
    }
  ]
}